{
    "name": "straylightagency/fonts",
    "description": "Helper to rend HTML tags to load fonts from services like Google Fonts, Adobe Fonts and Fontawesome",
    "keywords": [
        "fonts",
        "bunny-fonts",
        "google-fonts",
        "adobe-fonts",
        "fontawesome"
    ],
    "homepage": "https://github.com/straylightagency/php-fonts",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Anthony Pauwels",
            "email": "anthony@straylightagency.be",
            "homepage": "https://straylightagency.be",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Straylightagency\\Fonts\\": "src"
        }
    },
    "require": {
        "php":">=8.3.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Straylightagency\\Fonts\\Laravel\\FontsServiceProvider"
            ],
            "aliases": {
                "Fonts": "Straylightagency\\Fonts\\Laravel\\Fonts"
            }
        }
    }
}
